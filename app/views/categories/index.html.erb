
<%@categories.each do |category|%>
 <div class="card">
  <img src="jeans3.jpg" alt="Denim Jeans" style="width:100%">
  <h1>Tailored Jeans</h1>
  <p class="price"><%= category.name %></p>
  <p><%= link_to "show category", category_path(category)%></p>
  <p><%= link_to "delete category", category_path(category), method: :delete%></p> 
  </div> 
 <br>
<% end %>  
