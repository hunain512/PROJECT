<%@stores.each do |store|%>
  <div class="card">
   <img src="jeans3.jpg" alt="Denim Jeans" style="width:100%">
   <h1>Tailored Jeans</h1>
   <p class="price"><%= store.name %></p>
   <p><%= link_to "see store", store_path(store)%></p>
   <p><%= link_to "delete store", store_path(store), method: :delete %></p>
  </div> 
  <br>
<% end %>  