<% @users.each do |user|%>
  <div class="card">
   <img src="jeans3.jpg" alt="Denim Jeans" style="width:100%">
   <h2>user email </h2>
   <p class="price"><%= user.email %></p>
   <p><%= link_to "show user", user_path(user)%></p>
   <p><%= link_to "edit user", edit_user_path(user)%></p>
   <p><%= link_to "delete user", user_path(user), method: :delete%></p> 
   </div> 
  <br>
 <% end %>  
 